      },
      "name": "balanceView",
      "isExisting": false,
      "definition": "select \"involvements\".\"userId\", \"users\".\"displayName\", \"users\".\"profilePictureUrl\", SUM(\"involvements\".\"amount\") as \"netBalance\", \n      CASE\n        WHEN SUM(\"involvements\".\"amount\") < 0 THEN 'creditor'\n        WHEN SUM(\"involvements\".\"amount\") >= 0 THEN 'debtor'\n      END\n     as \"status\" from \"involvements\" inner join \"users\" on \"involvements\".\"userId\" = \"users\".\"id\" group by \"involvements\".\"userId\", \"users\".\"displayName\" order by 'netBalance' asc"
    }
