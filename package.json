{
  "name": "worker-test",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:native": "tauri dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "deploy": "nuxt build && npx wrangler --cwd .output deploy",
    "drizzle:generate": "npx drizzle-kit generate",
    "drizzle:migrate": "npx drizzle-kit migrate",
    "drizzle:migrate:local": "npx wrangler d1 migrations apply worker-test",
    "drizzle:studio": "npx drizzle-kit studio",
    "drizzle:seed": "npx dotenv-cli -v DATABASE_URL=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) -- npx tsx ./drizzle/seed.ts",
    "liam:generate": "tbls out -t json -o liam/schema.json $(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit)",
    "liam:view": "npx @liam-hq/cli erd build --format=tbls --input liam/schema.json && npx serve dist/"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.0",
    "@miniflare/d1": "^2.14.4",
    "@miniflare/shared": "^2.14.4",
    "@nuxt/icon": "^2.1.0",
    "@nuxt/ui": "^4.1.0",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/cli": "^2.9.4",
    "better-sqlite3": "^12.4.1",
    "drizzle-orm": "^0.44.7",
    "drizzle-seed": "^0.3.1",
    "groq-sdk": "^0.36.0",
    "nuxt": "^4.2.1",
    "nuxt-auth-utils": "^0.5.25",
    "typescript": "^5.9.3",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "drizzle-kit": "^0.31.7",
    "nitro-cloudflare-dev": "^0.2.2",
    "tsx": "^4.20.6",
    "wrangler": "^4.47.0"
  }
}
